# ISPW API Configuration
# Copy this file to .env and fill in your values

# CES Host and Port (for custom CES server)
CES_HOST=localhost
CES_PORT=2020

# Or use production server URL
# ISPW_BASE_URL=https://ispw.api.compuware.com

# Authentication - Personal Access Token for CES
ISPW_API_TOKEN=your_personal_access_token_here

# Default SRID (System Resource Identifier)
ISPW_DEFAULT_SRID=ISPW

# Request timeout in seconds
ISPW_TIMEOUT=30
